{
  "name": "@datorama/core",
  "version": "0.0.0-development",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "setup": "semantic-release-cli setup",
    "build": "gulp --gulpfile lib/build-tools/gulpfile.js",
    "commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "start": "cd playground && npm start",
    "link-bin": "link-parent-bin --child-directory-root .",
    "themes": "cd lib && npm run themify:dev",
    "test": "CI=true karma start lib/config/karma.conf.js",
    "test:local": "karma start lib/config/karma.conf.js",
    "pub": "npm run build && cd dist && npm publish --access public",
    "docs": "cd playground && npm run github-deploy:prod",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/npm",
        "pkgRoot": "dist"
      },
      "@semantic-release/git"
    ],
    "publish": [
      {
        "path": "@semantic-release/npm",
        "pkgRoot": "dist"
      }
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@datorama/postcss-node-sass": "^1.0.0",
    "@netbasal/spectator": "^1.11.0",
    "@semantic-release/changelog": "^2.0.1",
    "@semantic-release/git": "^4.0.1",
    "@semantic-release/npm": "^3.2.2",
    "@types/jasmine": "^2.8.7",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^8.1.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean-css": "^3.9.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-watch": "^5.0.0",
    "hex-to-rgba": "^1.0.1",
    "husky": "^0.14.3",
    "jasmine-core": "3.1.0",
    "json-loader": "^0.5.4",
    "karma": "1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-webpack": "^2.0.3",
    "link-parent-bin": "^0.2.3",
    "lint-staged": "^7.1.2",
    "marked": "^0.3.6",
    "moment": "^2.22.1",
    "path": "^0.12.7",
    "postcss": "^6.0.19",
    "prettier": "^1.11.1",
    "puppeteer": "^1.2.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.10",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.0",
    "run-sequence": "^2.2.1",
    "rxjs": "^6.3.2",
    "sass-json-export": "^2.0.0",
    "semantic-release": "^15.7.2",
    "sorcery": "^0.10.0",
    "sync-json": "^1.0.2",
    "through2": "^2.0.3",
    "to-string-loader": "^1.1.5",
    "travis-deploy-once": "^5.0.1",
    "ts-loader": "^3.0.5",
    "typescript": "2.7",
    "webpack": "3.8.1",
    "zone.js": "^0.8.26"
  },
  "dependencies": {
    "@angular/animations": "^6.0.6",
    "@angular/cdk": "^6.3.0",
    "@angular/cli": "1.6.5",
    "@angular/common": "^6.0.6",
    "@angular/compiler": "^6.0.6",
    "@angular/compiler-cli": "^6.0.6",
    "@angular/core": "^6.0.6",
    "@angular/forms": "^6.0.6",
    "@angular/http": "^6.0.6",
    "@angular/language-service": "^6.0.6",
    "@angular/platform-browser": "^6.0.6",
    "@angular/platform-browser-dynamic": "^6.0.6",
    "@angular/router": "^6.0.6",
    "@datorama/cli": "^1.2.4",
    "@datorama/themify": "1.0.5",
    "@datorama/utils": "^1.17",
    "ag-grid": "17.1.1",
    "ag-grid-angular": "^17.1.0",
    "ag-grid-enterprise": "^17.1.1",
    "angular-sortablejs": "^2.5.2",
    "global": "^4.3.2",
    "helpful-decorators": "^1.7.0",
    "ngx-take-until-destroy": "^5.0.0",
    "popper.js": "^1.14.3",
    "rxjs-compat": "^6.2.1",
    "sass-loader": "^6.0.7",
    "semantic-release-cli": "^4.0.7",
    "sortablejs": "^1.7.0",
    "style-loader": "^0.20.3",
    "tooltip.js": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datorama/client-core.git"
  }
}
